{
  "name": "pangwarta-rad-server",
  "version": "0.0.0",
  "description": "GraphQL server for Pangwarta the RAD admin template",
  "main": "index.ts",
  "repository": "https://bitbucket.org/pangwarta/pangwarta-rad",
  "author": "Pangwarta",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "prisma-client-lib": "^0.2.58",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "graphql": "^0.13",
    "graphql-tools": "^3.1.1",
    "graphql-yoga": "^1.16.2"
  },
  "scripts": {
    "dev": "ts-node $NODE_DEBUG_OPTION -O '{\"module\": \"commonjs\"}' --ignore false index.ts",
    "lint": "tslint -p .",
    "lint:watch": "watch 'yarn lint'",
    "precommit": "yarn lint && yarn typecheck",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:watch": "tsc --noEmit --skipLibCheck --watch"
  }
}
